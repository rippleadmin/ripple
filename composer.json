{
    "name": "rippleadmin/ripple",
    "description": "Quick and easy to customize admin panel for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Yang",
            "email": "yangchenshin77@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "admin",
        "customize",
        "generator"
    ],
    "homepage": "https://github.com/rippleadmin/ripple",
    "require": {
        "php": "^7.2.5|^8.0",
        "additionapps/flexible-presenter": "^2.0",
        "facade/ignition-contracts": "^1.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "inertiajs/inertia-laravel": "^0.2.5",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "laravel/ui": "^2.0|^3.0",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "RippleAdmin\\": "src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RippleAdmin\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RippleAdmin\\RippleServiceProvider"
            ],
            "aliases": {
                "Ripple": "RippleAdmin\\Ripple"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
